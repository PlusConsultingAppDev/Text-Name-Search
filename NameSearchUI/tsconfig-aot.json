{
	"compilerOptions": {
		"baseUrl": ".",
		"target": "es5",
		"module": "es2015",
		"moduleResolution": "node",
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"lib": [
			"es2015",
			"dom"
		],
		"noImplicitAny": true,
		"suppressImplicitAnyIndexErrors": true,
		"inlineSourceMap": false,
		"outDir": "temp/aot"
	},
	"files": [
		"src/app/app.module.ts",
		"src/main.ts",
		"src/framework.ts",
		"src/vendor.ts",
		"src/polyfills.ts",
		"src/tabs.ts"
	],
	"angularCompilerOptions": {
		"genDir": "temp/aot",
		"skipMetadataEmit": true
	}
}